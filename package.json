{
  "name": "der-nackte-halloumi",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:mock-server": "nodemon --watch mock-server ./mock-server",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "preexport": "yarn run build",
    "export": "next export",
    "test:watch": "jest --watch",
    "test:ci": "jest --coverage --ci",
    "eslint": "eslint --ext .tx,.tsx",
    "lint": "yarn run eslint src/",
    "tsc:lint": "tsc --noEmit -p .",
    "prettier": "prettier --write",
    "format": "yarn run prettier \"src/**/*.ts\" \"src/**/*.tsx\""
  },
  "dependencies": {
    "@types/react-virtualized-auto-sizer": "^1.0.0",
    "axios": "^0.19.2",
    "i18next": "^19.3.2",
    "i18next-browser-languagedetector": "^4.0.2",
    "linaria": "^1.3.3",
    "lodash": "^4.17.15",
    "next": "^9.3.3",
    "ramda": "^0.27.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-i18next": "^11.3.3",
    "react-map-gl": "^5.2.3",
    "react-virtualized-auto-sizer": "^1.0.2"
  },
  "devDependencies": {
    "@types/jest": "^25.1.3",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.7.1",
    "@types/ramda": "^0.27.0",
    "@types/react": "^16.9.20",
    "@types/react-map-gl": "^5.2.0",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "@typescript-eslint/parser": "^2.22.0",
    "@zeit/next-css": "^1.0.1",
    "babel-jest": "^25.2.4",
    "dotenv": "^8.2.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "^3.0.0",
    "fastify": "^2.14.0",
    "fastify-cors": "^3.0.3",
    "husky": "^4.2.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.2.4",
    "lint-staged": "^10.1.0",
    "nodemon": "^2.0.3",
    "prettier": "2.0.2",
    "typescript": "^3.7.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run tsc:lint && lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn run test --findRelatedTests --ci",
      "yarn run eslint --cache --fix"
    ]
  }
}
